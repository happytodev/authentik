{
    "name": "happytodev/authentik",
    "description": "A package for TempestPHP to handle OAuth authentication with providers like GitHub, Amazon and others to come.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Happytodev\\Authentik\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "league/oauth2-client": "^2.6",
        "league/oauth2-github": "^3.0",
        "tempest/framework": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "tempest/core": "^1.0",
        "tempest/auth": "^1.0"
    },
    "authors": [
        {
            "name": "Fred Blanc",
            "email": "happytodev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
